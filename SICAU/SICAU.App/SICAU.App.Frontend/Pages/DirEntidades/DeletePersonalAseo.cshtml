@page
@model SICAU.App.FrontEnd.Pages.DeletePersonalAseoModel
@{
}

<h3>Eliminar Personal de Aseo</h3>

<p></p>

<tr>
    <td>
        <a class="btn btn-primary table-btn" asp-page="/DirEntidades/ListPersonalAseo">
        <i class="fas fa-arrow-circle-left"></i> 
            Regresar
        </a>
    </td>
</tr>

<p></p>

<table class="table table-bordered table-striped">
    <tr>
        <td class="table-dark">ID PersonalAseo</td>
        <td>@Model.personalAseo.id</td>
    </tr>
    <tr>
        <td class="table-dark">Nombre PersonalAseo</td>  
         <td>@Model.personalAseo.nombre</td>      
    </tr>
    <tr>
   
        <td class="table-dark">Apellido PersonalAseo</td>
        <td>@Model.personalAseo.apellido</td>
    </tr>
    <tr>    
        <td class="table-dark">Identificacion PersonalAseo</td>
        <td>@Model.personalAseo.identificacion</td>
    </tr>     
</table>

<p></p>

<form method="post">
    <input type="hidden" asp-for="personalAseo.id">
    <button type="submit" class="btn btn-primary">
        <i class="far fa-trash-alt"></i>
        Eliminar
    </button>
</form>